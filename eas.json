{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "https://soultalkapp.com/api"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "API_BASE_URL": "https://soultalkapp.com/api"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "dev@soultalkapp.com",
        "ascAppId": "6759283325",
        "appleTeamId": "7ZL49383C4"
      }
    }
  }
}
